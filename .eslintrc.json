{
  "env": {
    "es2020": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "rules": {
    "no-undef": "off",
    "no-console": "off",
    "no-multi-assign": "off",
    "prefer-template": "off",
    "no-plusplus": "off",
    "no-unused-vars": "off",
    "no-use-before-define": "off"
  }
}
